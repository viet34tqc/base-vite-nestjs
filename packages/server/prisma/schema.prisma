// MongoDB datasource
// To use PostgreSQL, comment out the MongoDB datasource and uncomment the PostgreSQL one below

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

// datasource db {
//   provider = "postgresql"
//   url      = env("POSTGRES_URL")
// }

// Example model
model Example {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  createdAt DateTime @default(now())
}
